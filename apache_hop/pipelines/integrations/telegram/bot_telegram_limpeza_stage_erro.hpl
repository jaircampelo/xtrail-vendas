    <sql>with cte_log_erro as
(

SELECT '%E2%9A%A0%EF%B8%8F%20Erro%20de%20execu%C3%A7%C3%A3o%20na%20limpeza%20da%20Stage%20Area%0A%0A' 	as titleLogging
, REPLACE(
		   REPLACE(
				    REPLACE(
						     REPLACE(
									  REPLACE(
											   REPLACE(
														REPLACE(
																 REPLACE(
																		  REPLACE(
																				   REPLACE(
																						    REPLACE(
																								     REPLACE(
																										      REPLACE(
																												       REPLACE(
																														        REPLACE(
																																         REPLACE(
																																		          REPLACE(											
																																				           REPLACE(										
																																						            REPLACE(
																																								             cast(workflowLogging as VARCHAR(MAX))
																																								           , '\','\\')
																																						            , '_','\_')
																																						 , '*','\*')
																																			    , '[','\[')
																																	   , ']','\]')
																															  , '(','\(')
																													 , ')','\)')
																										    , '~','\~')
																								   , '`','\`')
																						  , '>','\>')
																			     , '#','\#')
																	    , '+','\+')
															   , '-','\-')
													  , '=','\=')
											 , '|','\|')
								    , '{','\{')
						   , '}','\}')
				  , '.','\.')
		 , '!','\!') 																																					as workflowLogging
, 'markdownv2' 																																							as parseMode
, loggingDate
FROM logOrquestracaoDw

)

select top 1
	   a.titleLogging																								as titleMessage
	 , '%F0%9F%95%90%20Hor%C3%A1rio%20de%20ocorr%C3%AAncia%20do%20erro%3A%20' 										as titleDateTime
	 , '`' + convert(varchar(50),b.loggingDate,121) + '`' 															as dateTime
	 , '%0A%0A%F0%9F%97%BA%EF%B8%8F%20Local%20de%20ocorr%C3%AAncia%20do%20erro%3A%0A' 								as titleLocalName
	 , '`' + b.actionName + '`'																						as localName
	 , '%0A%0A%F0%9F%93%8D%20Rastreamento%20de%20erro%20no%20log%20de%20transa%C3%A7%C3%A3o%3A%0A%0A'				as titleWorkflowLogging
	 , replace( replace( replace( substring( a.workflowLogging
						 			       , charindex(replace(b.actionName,'_','\_'),a.workflowLogging)
						 			       , 4000)
					            , CHAR(13) + CHAR(10), '%0A')
			  		   , CHAR(13), '%0A')
			  , CHAR(10), '%0A')																					as workflowLogging
	 , a.parseMode 																									as parseMode
  from cte_log_erro 	 a
  left
  join logOrquestracaoDw b on a.loggingDate = b.loggingDate
 where left(b.workflowName,5) in ('carga','limpa')
   and b.actionErrors >=1
 order
    by b.loggingDate desc</sql>
