    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT 
    CASE Mes
        WHEN 'Janeiro'   THEN CONCAT(Ano,'-01-','01')
        WHEN 'Fevereiro' THEN CONCAT(Ano,'-02-','01')
        WHEN 'Março'     THEN CONCAT(Ano,'-03-','01')
        WHEN 'Abril'     THEN CONCAT(Ano,'-04-','01')
        WHEN 'Maio'      THEN CONCAT(Ano,'-05-','01')
        WHEN 'Junho'     THEN CONCAT(Ano,'-06-','01')
        WHEN 'Julho'     THEN CONCAT(Ano,'-07-','01')
        WHEN 'Agosto'    THEN CONCAT(Ano,'-08-','01')
        WHEN 'Setembro'  THEN CONCAT(Ano,'-09-','01')
        WHEN 'Outubro'   THEN CONCAT(Ano,'-10-','01')
        WHEN 'Novembro'  THEN CONCAT(Ano,'-11-','01')
        WHEN 'Dezembro'  THEN CONCAT(Ano,'-12-','01')
     END 		AS meta_data
   , Valor     	AS meta_valor
   , GETDATE() 	AS meta_data_registro
FROM bronzeMetas</sql>
